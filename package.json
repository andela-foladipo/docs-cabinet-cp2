{
  "name": "docs-cabinet-cp2",
  "version": "0.0.1",
  "description": "A beautiful and fast document management system.",
  "main": "server.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "build:watch": "./node_modules/.bin/webpack --watch",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:ops": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed:all",
    "db:seed:all": "./node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo:all": "./node_modules/.bin/sequelize db:seed:undo:all",
    "deploy": "NODE_ENV=production npm run db:ops && npm run build && NODE_ENV=production SEQUELIZE_LOGGING=false npm start",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "start": "SEQUELIZE_LOGGING=false ./node_modules/.bin/babel-node server.js",
    "start:dev": "./node_modules/.bin/nodemon server.js --exec babel-node --presets es2015,react,node6",
    "test": "npm run test:server",
    "test:nyc": "SEQUELIZE_LOGGING=false ./node_modules/.bin/nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --timeout 5000 --require babel-register ./server/tests/*/*[sS]pec.js",
    "test:server": "SEQUELIZE_LOGGING=false ./node_modules/.bin/mocha --timeout 5000 --require babel-register ./server/tests/*/*[sS]pec.js",
    "test:travis": "NODE_ENV=test npm run test:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-foladipo/docs-cabinet-cp2.git"
  },
  "keywords": [
    "document",
    "management",
    "react",
    "postgres"
  ],
  "author": "Folusho Oladipo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-foladipo/docs-cabinet-cp2/issues"
  },
  "homepage": "https://github.com/andela-foladipo/docs-cabinet-cp2#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "jsonwebtoken": "^7.4.1",
    "materialize-css": "^0.98.2",
    "mysql": "^2.13.0",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-materialize": "^1.0.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "swagger-jsdoc": "^1.9.5",
    "uuid": "^3.1.0",
    "webpack": "^2.6.1"
  }
}
