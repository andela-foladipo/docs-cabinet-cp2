{
  "name": "docs-cabinet-cp2",
  "version": "0.0.1",
  "description": "A beautiful and fast document management system.",
  "main": "server.js",
  "scripts": {
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "./node_modules/.bin/sequelize db:seed:undo:all",
    "start": "./node_modules/.bin/babel-node server.js",
    "start:dev": "./node_modules/.bin/nodemon server.js --exec babel-node --presets es2015,react,node6",
    "test": "npm run test:server",
    "test:server": "./node_modules/.bin/mocha --require babel-register server/tests/*/*[sS]pec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-foladipo/docs-cabinet-cp2.git"
  },
  "keywords": [
    "document",
    "management",
    "react",
    "postgres"
  ],
  "author": "Folusho Oladipo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-foladipo/docs-cabinet-cp2/issues"
  },
  "homepage": "https://github.com/andela-foladipo/docs-cabinet-cp2#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "path": "^0.12.7",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "webpack": "^2.6.1"
  }
}
